{
  "compilerOptions": {
    "traceResolution": true,
    "strictNullChecks": true,
    "outDir": "./dist/",
    "sourceMap": true,
    "noImplicitAny": false,
    "suppressImplicitAnyIndexErrors": true,
    "module": "commonjs",
    "target": "es5",
    "jsx": "react",
    "lib": ["dom", "es6", "dom.iterable", "scripthost"],
    "baseUrl": "server/"
  },
  "include": ["./server/**/*"],
  "awesomeTypescriptLoaderOptions": {
    "silent": true
  }
}
